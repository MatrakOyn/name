local Section = Tab:NewSection("Trade Scam")
Section:NewButton("Start", "ButtonInfo", function()
    print("Clicked")
end)
Section:NewButton("Stop", "ButtonInfo", function()
    print("Clicked")
end)
local Tab = Window:NewTab("Settings")
